#!/bin/sh
# 2013/12/02, sunsd

f=$1
module="webframe"
if [ "${f:0:1}" != "/" ]; then
    f="`pwd`/$f"
fi
dirname=${f/$module*/$module}
if [ "$dirname" != "$f" ]; then
    line=`grep -m 1 "^INSTALLDIR" $dirname/Makefile`
    INSTDIR="$CCRUN/htdocs/${line#*/}"
    cmd=".dosu cp $f $INSTDIR/${f#*webframe/}"
    echo $cmd
    eval $cmd
    if [ $? -eq 0 ]; then
        echo "[7mDone.[0m"
    fi
else
    echo "Path is not valid!"
fi
